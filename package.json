{
  "name": "recipe-search-app",
  "version": "1.0.0",
  "description": "A recipe search application based on ingredients using PostgreSQL and Express",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db": "node scripts/init-db.js",
    "seed-db": "node scripts/seed-db.js",
    "seed-csv": "node scripts/seed-csv-reliable.js",
    "reset-db": "node scripts/reset-db.js",
    "dbt-deps": "dbt deps",
    "dbt-seed": "dbt seed",
    "dbt-run": "dbt run",
    "dbt-test": "dbt test",
    "dbt-compile": "dbt compile",
    "dbt-build": "dbt build",
    "dbt-setup": "npm run dbt-deps && npm run init-db && npm run dbt-seed"
  },
  "keywords": [
    "recipes",
    "search",
    "postgresql",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "mysql2": "^3.6.5",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
